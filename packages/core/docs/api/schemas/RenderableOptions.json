      "enum": [
        "auto",
        "flex-start",
        "center",
        "flex-end",
        "stretch",
        "baseline",
        "space-between",
        "space-around",
        "space-evenly"
      ],
      "type": "string"
    },
    "FlexDirectionString": {
      "enum": [
        "column",
        "column-reverse",
        "row",
        "row-reverse"
      ],
      "type": "string"
    },
